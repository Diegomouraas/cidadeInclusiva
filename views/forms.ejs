<section class="container">
    <h3>Cadastro Empresas</h3>
    <form class="row col-md-4">
        <!-- <form action="#" method="POST">-->
        <div class="form-group">
            <label for="nome">Nome: </label>
            <input type="text" name="nome" placeholder="Seu nome aqui" class="form-control">
        </div>

        <div class="form-group">
            <label for="telefone">Telefone: </label>
            <input type="number" name="telefone" placeholder="(xx)xxxxxxxxx" class="form-control">
        </div>

        <div class="form-group">
            <label for="email">Email: </label>
            <input type="email" name="email" placeholder="example@example.com" class="form-control">
        </div>

        <div class="form-group">
            <label for="cep">CEP: </label>
            <input type="number" name="cep" placeholder="xxxxx-xxx" class="form-control">
        </div>

        <div class="form-group">
            <label for="bairro">bairro: </label>
            <input type="text" name="bairro" placeholder="bairro aqui" class="form-control">
        </div>

        <div class="form-group">
            <label for="logradouro">Logradouro: </label>
            <input type="text" name="logradouro" placeholder="logradouro aqui" class="form-control">
        </div>

        <div class="form-group">
            <label for="numero">Numero: </label>
            <input type="number" name="numero" placeholder="xx" class="form-control">
        </div>
    </form>
    <br>
    <button type="submit" class="btn btn-primary col-sm-1 align-center">Enviar</button>

</section>

</br>