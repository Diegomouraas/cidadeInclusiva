<!DOCTYPE html>
<html lang="pt-br">

    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link href="css/styles.css" rel="stylesheet" type="text/css" media="all" />
        <link href="css/bootstrap.css" rel="stylesheet" type="text/css" media="all" />
        <title>Angra Cidade Inclusiva</title>
        <link rel="shortcut icon" href="img/faviicon.ico" type="image/x-icon"/>
    </head>

<body>
    <%- include ('frags/header.ejs') %>
   <br><br><br><br>
    <%- include ('frags/formCadastro.ejs') %>
    <%- include ('frags/footer.ejs') %> 
    <script src="js/bootstrap.js"></script>

    <!-- autoformatação do cadastro -->

    <script src="js/jquery-1.9.0.min.js" type="text/javascript" charset="utf-8"></script>
    <script src="js/jquery.maskedinput.js" type="text/javascript"></script>
    <script type="text/javascript">
    $(function() {
        $.mask.definitions['~'] = "[+-]";
        $("#date").mask("99/99/9999",{placeholder:"mm/dd/yyyy",completed:function(){alert("completed!");}});
        //para celular
        $(".phone").mask("(99)99999-9999");
        $("#phoneExt").mask("(999) 999-9999? x99999");
        $("#iphone").mask("(99) 99999-9999");
        //para cep
        $("#tin").mask("99999-999");
        //para cpf
        $("#ssn").mask("999.999.999-99");
        $("#product").mask("a*-999-a999", { placeholder: " " });
        $("#eyescript").mask("~9.99 ~9.99 999");
        $("#po").mask("PO: aaa-999-***");
        $("#pct").mask("99%");
        $("#phoneAutoclearFalse").mask("(999) 999-9999", { autoclear: false, completed:function(){alert("completed autoclear!");} });
        $("#phoneExtAutoclearFalse").mask("(999) 999-9999? x99999", { autoclear: false });

        $("input").blur(function() {
            $("#info").html("Unmasked value: " + $(this).mask());
        }).dblclick(function() {
            $(this).unmask();
        });
    });

</script>
</body>

</html>
